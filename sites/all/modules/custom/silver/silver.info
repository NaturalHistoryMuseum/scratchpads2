name = Silver :: Scratchpad Import Verification
description = "Checks the format of import files to ensure that they match the templates provided, and also converts XLS/XLSX to a format that PHP can handle."
package = "Scratchpads"
core = 7.x
dependencies[] = libraries
dependencies[] = feeds
dependencies[] = feeds_tamper
dependencies[] = feeds_excel
dependencies[] = subform

features[ctools][] = "feeds:feeds_importer_default:1"